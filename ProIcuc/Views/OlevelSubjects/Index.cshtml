@model IEnumerable<ProIcuc.Models.Applications.OlevelSubject>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Doc
        </th>
        <th>
            Subject
        </th>
        <th>
            Grade
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Applicant.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Applicant.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OlevelDoc.DocName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OlevelSubjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grading)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OlevelSubjectID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OlevelSubjectID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OlevelSubjectID })
        </td>
    </tr>
}

</table>
